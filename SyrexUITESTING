--Syrex UI Library
local SyrexUI = loadstring(game:HttpGet("https://raw.githubusercontent.com/Joshingtonn123/JoshScript/refs/heads/main/SyrexUITESTING"))()

-- Configure UI appearance (optional)
SyrexUI:UI(
    "Syrex Hub", -- Title
    "Premium Script Hub", -- Subtitle
    24, -- Title size
    14  -- Subtitle size
)

-- Key System with custom settings
SyrexUI:KeySystem(
    true, -- Enable key system (true/false)
    "https://your-discord-link-here.com", -- Your custom link
    "1234", -- Your custom key
    "Join our Discord for the key!" -- Your custom note
)

-- Create window with title and subtitle
local window = SyrexUI:CreateWindow("Syrex Hub", "Premium Script Hub")

-- Tab 1
local tab1 = window:CreateTab("Tab1")

tab1:AddToggle("Toggle", false, function(state)
    -- put function here
    if state then
        print("Toggle enabled!")
    else
        print("Toggle disabled!")
    end
end)

tab1:AddButton("Button", function()
    -- put function here
    print("Teleporting to base...")
end)

tab1:AddToggleKeybind("GUI Toggle Key")

-- Tab 2
local tab2 = window:CreateTab("Tab2")

tab2:AddSlider("Slider", 16, 200, 16, function(val)
    -- put function here
    print("Slider set to:", val)
end)

tab2:AddNumberBox("Number Box", 50, function(value)
    -- put function here
    print("Number box set to:", value)
end)

-- Tab 3
local tab3 = window:CreateTab("Tab3")

tab3:AddLabel("Made by Josh")
tab3:AddLabel("Version 2.0")
tab3:AddLabel("Enjoy the UI library")

tab3:AddKeybind("Noclip Key", Enum.KeyCode.N, function(key)
    -- put function here
    print("Noclip key set to:", key)
end)

tab3:AddKeybind("Speed Key", Enum.KeyCode.B, function(key)
    -- put function here
    print("Speed key set to:", key)
end)
